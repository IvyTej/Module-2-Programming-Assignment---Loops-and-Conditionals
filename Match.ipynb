    "# True or False assigned to small and green\n",
    "small = True\n",
    "green = False\n",
    "\n",
    "# determine item matches the fruit\n",
    "if small and green:\n",
    "    print(\"pea\")\n",
    "elif small and not green:\n",
    "    print(\"cherry\")\n",
    "elif not small and green:\n",
    "    print(\"watermelon\")\n",
    "else:\n",
    "    print(\"pumpkin\")"
